{
  "stats": {},
  "log": { "loglevel": "warning" },

  "policy": {
    "system": {
      "statsOutboundUplink": true,
      "statsOutboundDownlink": true
    }
  },

  "inbounds": [
    {
      "tag": "socks",
      "protocol": "socks",
      "port": 0,
      "listen": "",
      "settings": {
        "auth": "noauth",
        "udp": true,
        "userLevel": 8
      },
      "sniffing": {
        "enabled": true,
        "routeOnly": false,
        "destOverride": ["http", "tls"]
      }
    }
  ],

  "outbounds": [
    {
      "tag": "proxy",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "",
            "port": 0,
            "users": [
              {
                "id": "",
                "alterId": 0,
                "security": "auto",
                "level": 8
              }
            ]
          }
        ]
      },
      "streamSettings": {},
      "mux": { "enabled": false }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": { "domainStrategy": "UseIP" }
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": { "response": { "type": "http" } }
    }
  ],

  "routing": {
    "domainStrategy": "AsIs",
    "rules": []
  },

  "dns": {
    "hosts": {},
    "servers": []
  }
}